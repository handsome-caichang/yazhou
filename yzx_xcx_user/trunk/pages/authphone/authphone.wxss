/* 微信授权登录 */
.auth-box {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    background-color: #ffffff;
    z-index: 10;
    padding: 0 30px;
}
.auth-logo {
    width: 63px;
    height: 63px;
    background-size: 63px;
    margin: 59px auto 15px auto;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
}
.auth-title {
    font-size: 19px;
    padding-bottom: 25px;
}
.auth-split {
    border-bottom: 1px solid #EFEFEF;
}
.auth-describe1 {
    padding: 25px 0 16px 0;
    color: #5A5A5A;
    font-size: 16px;
}
.auth-describe2 {
    color: #909090;
    font-size: 14px;
}
.auth-btn {
    margin-top: 44px;
    height: 45px;
    line-height:45px;
    font-size: 15px;
    color: #FFFFFF;
    background-color: #52BA37;
}
