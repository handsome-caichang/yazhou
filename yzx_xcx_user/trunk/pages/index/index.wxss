body {
    border: 0;
    margin: 0;
    outline: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    -webkit-user-select: none;
    -webkit-touch-callout: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-text-size-adjust: none;
    font: 16px/1.25 "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif, "微软雅黑";
}

/* 微信授权登录 */
.auth-box,
.exit-box,
.loading-box {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    background-color: #ffffff;
    z-index: 10;
}
.auth-logo {
    width: 75px;
    height: 75px;
    background-size: 75px;
    margin: 85px auto 20px auto;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
}
.auth-title {
    font-size: 24px;
    padding-bottom: 40px;
}
.auth-describe {
    padding-top: 30px;
    padding-bottom: 10px;
    color: #999999;
    font-size: 14px;
}
.auth-btn-wrap {
    padding: 15px 45px;
}
.auth-btn {
    height: 45px;
    background-color: #1AAD19;
    color: #FFFFFF;
}

/* 加载中 */
.loading-box {
    display: flex;
    align-items: center;
    justify-content: center;
}
.loading {
    width: 40px;
    height: 40px;
    background-size: 40px;
    background-image: url("data:image/gif;base64,")
}
/* 页面回退 */
.exit-box {
    padding: 0 35px;
}
.exit-logo {
    width: 220px;
    height: 220px;
    background-size: 220px;
    margin: 66px auto 25px auto;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
}
.exit-box .exit-tips{
    color: #999999;
    font-size: 14px;
    text-align: center;
}
.exit-box .exit-btn{
    height: 45px;
    line-height: 45px;
    border-radius: 34px;
    text-align: center;
    color: #FFFFFF;
    font-size: 16px;
    margin-top: 25px;
	opacity: 1;
    background-color: #FF8775;
}

.exit-btn.change{
    background-color: #1E88F5;
}
.exit-btn.goto-index{
    background-color:rgb(0,178,106);
}