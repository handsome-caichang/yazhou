body {
    border: 0;
    margin: 0;
    outline: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    -webkit-user-select: none;
    -webkit-touch-callout: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-text-size-adjust: none;
    font: 16px/1.25 "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif, "微软雅黑";
}
.login {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    overflow: auto;
    padding: 125px 33px 0 33px;
    background-color: #ffffff;
}
.login .icon {
    width: 1em;
    height: 1em;
    vertical-align: -0.15em;
    fill: currentColor;
    overflow: hidden;
}
.login input {
    width: 100%;
    height: 44px;
    border: none;
    -webkit-appearance: none;
    -webkit-user-select: text;
    -webkit-font-smoothing: antialiased;
    -wekbit-box-sizing: border-box;
    box-sizing: border-box;
    outline: none;
    font-size: 16px;
}
.login .placeholder{
    color: #C7C7CC;
}

/*登录表单*/
.login .form-group {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    position: relative;
    height: 44px;
    /*line-height: 44px;*/
    /*border-bottom:1px solid #F5F5F5;*/
}
.login .form-group:first-child {
    margin-bottom: 52px;
}
.login .form-group .form-group-left {
    display: flex;
    position: relative;
    flex: 1;
    padding-left: 30px;
}
.login .form-group .form-group-left .name-input {
    flex: 1;
}
.login .form-group .form-group-left .company-name-wrapper {
    padding: 10px 0;
    width: 90px;
    font-size: 14px;
    color: #999;
}
.login .form-group .form-group-left .company-name-wrapper .company-name {
    height: 20px;
    border-left: 1px solid #999;
    text-align: center;    
}


.login .form-group .form-group-left .icon {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 5px;
    width: 15px;
    height: 15px;
    margin: auto;
    background-size: 15px;
    background-repeat: no-repeat;
}
.login .form-group .icon-yonghumingicon {
    background-image: url(data:image/png;base64,);
}
.login .form-group .icon-mimaicon {
    background-image: url(data:image/png;base64,);
}
.login .form-group .icon-xianshi,
.login .form-group .icon-buxianshi {
    width: 16px;
    height: 16px;
    background-size: 16px;
    background-repeat: no-repeat;
}
.login .form-group .icon-xianshi {
    background-image: url("data:image/svg+xml;base64,");
}
.login .form-group .icon-buxianshi {
    background-image: url("data:image/svg+xml;base64,");
}
.login .form-group .form-group-right {
    position: relative;
    white-space: nowrap;
    width: 50px;
    display: flex;
    display: -webkit-box;
    display: -ms-flexbox;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}
.login .form-group .form-error-tips{
    position: absolute;
    top: 130%;
    left: 30px;
    font-size: 13px;
    line-height: 1;
    color: #FF4241;
}
.login .form-btn {
    background-color: #F03232;
    height: 45px;
    line-height: 45px;
    border-radius: 6px;
    text-align: center;
    color: #FFFFFF;
    opacity: 0.3;
    font-size: 16px;
    margin-top: 30px;
}
.login .form-btn.active {
    opacity: 1;
}
.login .form-btn.exit{
    opacity: 1;
    margin-top: 15px;
    background-color: #FF8775;
}
.login .exit-tips{
    color: #999999;
    font-size: 13px;
    margin-top: 5px;
    text-align: center;
}
.login .forget-password-container {
    margin-top: 15px;
    display: flex;
    font-size: 13px;
    height: 17px;
    line-height: 17px;
    color: #333;
}
.login .forget-password-container .register {
    color: #F03232;
    margin-left: 10px;
}
.login .forget-password-container .forget-password {
    display: inline-block;
    flex: 1;
    color: #F03232;
    text-align: right;
}


/* 微信授权登录 */
.auth-box,
.exit-box {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    background-color: #ffffff;
    z-index: 10;
}
.auth-logo {
    width: 75px;
    height: 75px;
    background-size: 75px;
    margin: 85px auto 20px auto;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
}
.auth-title {
    font-size: 24px;
    padding-bottom: 40px;
}
.auth-describe {
    padding-top: 30px;
    padding-bottom: 10px;
    color: #999999;
    font-size: 14px;
}
.auth-btn-wrap {
    padding: 15px 45px;
}
.auth-btn {
    height: 45px;
    background-color: #1AAD19;
    color: #FFFFFF;
}


/* 页面回退 */
.exit-box {
    padding: 0 35px;
}
.exit-logo {
    width: 220px;
    height: 220px;
    background-size: 220px;
    margin: 66px auto 25px auto;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
}
.exit-box .exit-tips{
    color: #999999;
    font-size: 14px;
    text-align: center;
}
.exit-box .exit-btn{
    height: 45px;
    line-height: 45px;
    border-radius: 34px;
    text-align: center;
    color: #FFFFFF;
    font-size: 16px;
    margin-top: 25px;
	opacity: 1;
    background-color: #FF8775;
}

.exit-btn.change{
    background-color: #1E88F5;
}
.exit-btn.goto-index{
    background-color:rgb(0,178,106);
}