.register-wrapper {
    padding: 60px 33px 0 33px;
}

.register-wrapper .phone {
    height: 44px;
    font-size: 14px;
    line-height: 44px;
    border-radius: 6px;
    border: 1px solid #F03232;
    padding-left: 15px;
}

.register-wrapper .identifying-code-wrapper {
    margin-top: 20px;
    display: flex;
}
.register-wrapper .identifying-code-wrapper .identifying-code {
    flex: 1;
    padding-left: 15px;
    height: 44px;
    font-size: 14px;
    line-height: 44px;
}
.register-wrapper .identifying-code-wrapper .get-code {
    width: 95px;
    color: #F03232;
    font-size: 13px;
    padding: 12px 0;
}
.register-wrapper .identifying-code-wrapper .get-code .code {
    border-left: 1px solid #999;
    text-align: center;
}

.register-wrapper .next-btn {
    margin-top: 60px;
    height: 44px;
    line-height: 44px;
    color: #FFF;
    text-align: center;
    background-color: #F03232;
    border-radius: 6px;
    font-size: 16px;
}

.register-wrapper .text {
    display: flex;
    margin-top: 20px;
    font-size: 12px;
    color: #999;
}
.register-wrapper .text .left {
    flex: 1;
    text-align: right;
}
.register-wrapper .text .right {
    color: #F03232;
    margin-left: 5px;
    text-decoration: underline;
}
.register-wrapper .tips {
    font-size: 12px;
    color: #999;
    display: flex;
}
.register-wrapper .icon {
    width: 15px;
    height: 15px;
    background-size: 15px;
    background-repeat: no-repeat;
}
.register-wrapper .tips .icon-tips {
    background-image: url(data:image/png;base64,)
}
.register-wrapper .tips .tips-text {
    flex: 1;
    padding-left: 5px;
}

.register-wrapper .pic-identifying {
    height: 44px;
    display: flex;
    margin-top: 20px;
}
.register-wrapper .pic-identifying .pic-left {
    flex: 1;
    height: 44px;
    line-height: 44px;
    padding-left: 15px;
    font-size: 17px;
}

.register-wrapper .pic-identifying .pic-mid {
    width: 99px;
    height: 44px;
}
.register-wrapper .pic-identifying .pic-right {
    width: 50px;
    height: 44px;
    font-size: 12px;
    padding: 12px 0;
}
.register-wrapper .pic-identifying .pic-right .pic-right-text {
    border-left: 1px solid #999;
    text-align: center;
}
.register-wrapper .campus {
    margin-top: 20px;
    height: 44px;
    line-height: 44px;
    display: flex;
}
.register-wrapper .campus .name {
    flex: 1;
    font-size: 14px;
    color: #999;
    padding-left: 15px;
}
.register-wrapper .campus .icon-xiala {
    margin-top: 14.5px;
    background-image: url(data:image/png;base64,);
}
