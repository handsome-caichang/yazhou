.item {
    height: 44px;
    line-height: 44px;
    display: flex;
    padding: 0 12px;
    font-size: 13px;
    color: #474747;
}
.item .item-left {
    flex: 1;
}
.item .item-right {
    width: 15px;
    height: 15px;
    background-size: 15px;
    background-repeat: no-repeat;
    margin-top: 14.5px;
}
.item .active {
    color: #f03232;
}
.item .icon {
    background-image: url(data:image/png;base64,);
}